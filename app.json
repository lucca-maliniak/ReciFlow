{
  "expo": {
    "name": "ReciFlow",
    "slug": "ReciFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/reciflow.png",
    "scheme": "reciflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/reciflow.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/reciflow.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBdjzLiPpll_cjX4QKf57Ho2GHXA2O4VZ0"
        }
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/reciflow.png"
    },
    "plugins": [
      "expo-router",
      "expo-maps"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
